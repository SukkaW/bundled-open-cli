{
  "name": "bundled-open-cli",
  "version": "1.0.1-beta.1",
  "private": false,
  "description": "Open stuff like URLs, files, executables. Cross-platform.",
  "repository": "https://github.com/SukkaW/bundled-open-cli",
  "main": "dist/cli.mjs",
  "bin": {
    "open": "dist/cli.mjs",
    "open-cli": "dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --format=sukka",
    "build": "rollup -c rollup.config.ts --configPlugin swc3 --bundleConfigAsCjs",
    "release": "bumpp -r --all --commit \"release: %s\" --tag \"%s\""
  },
  "keywords": [
    "cli-app",
    "cli",
    "app",
    "open",
    "opener",
    "opens",
    "launch",
    "start",
    "xdg-open",
    "xdg",
    "default",
    "cmd",
    "browser",
    "editor",
    "executable",
    "exe",
    "url",
    "urls",
    "arguments",
    "spawn",
    "exec",
    "child",
    "process",
    "website",
    "file"
  ],
  "author": "SukkaW <https://skk.moe>",
  "license": "MIT",
  "devDependencies": {
    "@eslint-sukka/node": "^6.23.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/node": "^24.3.1",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "eslint-config-sukka": "^6.23.1",
    "eslint-formatter-sukka": "^6.23.1",
    "open-cli": "^8.0.0",
    "rollup": "^4.50.0",
    "rollup-plugin-swc3": "^0.12.1",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=18"
  }
}
